{
  "name": "egepan",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "build_apk": "cd android && ./gradlew assembleRelease && cd ..",
    "build_aab": "cd android && ./gradlew bundleRelease && cd ..",
    "test:watch": "jest --watchAll --verbose",
    "test": "jest --verbose",
    "coverage": "jest --collectCoverage",
    "postinstall": "patch-package && npm-license-crawler -onlyDirectDependencies -json ./src/assets/files/licenses.json",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'",
    "clean_install_macos": "watchman watch-del-all && rm -rf node_modules && npm i --legacy-peer-deps && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd ..",
    "clean_install_windows": "npm i rimraf --no-save && npx rimraf node_modules && npm i --legacy-peer-deps",
    "lint": "eslint  . --ext .js,.jsx",
    "lint:fix": "eslint  . --ext .js,.jsx --fix",
    "lint:report": "eslint . --ext .js,.jsx --output-file eslint_report.json --format json ."
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^6.3.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "^4.3.0",
    "@react-native-firebase/app": "^17.3.2",
    "@react-native-firebase/messaging": "^17.3.2",
    "@react-native-picker/picker": "^2.4.3",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "node-forge": "^1.3.1",
    "react": "^17.0.1",
    "react-native": "^0.68.3",
    "react-native-elements": "^3.4.3",
    "react-native-encrypted-storage": "^4.0.2",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-modal": "^13.0.1",
    "react-native-permissions": "^3.6.0",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.15.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": ">=11.23.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@react-native-community/eslint-config": "3.0.2",
    "@testing-library/react-native": "^11.0.0",
    "babel-jest": "27.5.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^26.8.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "jest": "^27.5.1",
    "jetifier": "^2.0.0",
    "jsdom": "20.0.0",
    "metro-react-native-babel-preset": "^0.72.0",
    "patch-package": "^6.4.7",
    "react-test-renderer": "17.0.2"
  },
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
